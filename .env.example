# SSH Configuration
VM_HOST=your-kali-vm-ip
VM_PORT=22
VM_USERNAME=kali
VM_PASSWORD=your-password

# RDP Configuration
RDP_HOST=your-kali-vm-ip
RDP_PORT=3389
RDP_USERNAME=kali
RDP_PASSWORD=your-password

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key

# Network Configuration
NETWORK_SUBNET=192.168.1.0/24
NETWORK_GATEWAY=192.168.1.1
NETWORK_INTERFACE=eth0

# Traffic Baselines
BASELINE_RX_PPS=10.0
BASELINE_TX_PPS=15.0

# Common Protocols and Ports
COMMON_TCP_PORTS=22,80,443,3389
COMMON_UDP_PORTS=53,67,68,161

# Timeouts
TCP_FIN_TIMEOUT=60
APACHE_TIMEOUT=300
KEEPALIVE_TIMEOUT=5

# Security Status
IDS_PRESENT=false
WAF_PRESENT=false
FIREWALL_DEFAULT_POLICY=ACCEPT

# Monitoring Tools
AVAILABLE_TOOLS=tcpdump,nmap,wireshark
ACTIVE_MONITORING=false
SCHEDULED_MONITORING=false

# Rate Limiting Status
API_RATE_LIMITS=false
DNS_RATE_LIMITS=false
SCAN_RATE_LIMITS=false

# Scan Strategy Optimization
MAX_SCAN_RATE=50
RECOMMENDED_SCAN_INTERVAL=0
STEALTH_MODE_REQUIRED=false
SAFE_SCAN_WINDOW=any
